    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <dss_productattachment>
            <general>
                <enabled>0</enabled>
                <debug>0</debug>
            </general>
            <attachment>
                <tab_label>Attachments</tab_label>
